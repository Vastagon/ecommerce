generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model items {
  id Int @id
  title String
  price Float
  description String
  category String
  image String
  rating Float
  count Float
}

model users {
  id       String @id @default(cuid())
  email String @unique
  username String
}
